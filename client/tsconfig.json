{
  "compilerOptions": {
    "outDir": "../public/",
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
    },
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
  },
  "include": [
    "./**/*",
  ],
  "exclude": [
    "./node_modules",
  ],
}